{
    "name": "daiv/laravel-notify",
    "description": "Flexible flash notifications for Laravel",
    "keywords": [
        "Laravel",
        "laravel-notify",
        "laravel-notify-x",
        "notification"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/daiv05/laravel-notify-x/issues",
        "source": "https://github.com/daiv05/laravel-notify-x"
    },
    "authors": [
        {
            "name": "Arthur Monney",
            "email": "monneylobe@gmail.com"
        },
        {
            "name": "David Deras",
            "email": "davidderas50@gmail.com"
        }
    ],
    "require": {
        "php": "~8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.5",
        "laravel/pint": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Daiv\\Notify\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daiv\\Notify\\LaravelNotifyServiceProvider"
            ],
            "aliases": {
                "Notify": "Daiv\\Notify\\Facades\\LaravelNotify"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
